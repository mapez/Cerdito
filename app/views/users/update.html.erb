<%= will_paginate(@ausers) %>  
<div class = 'fondo'>
<div class = ' grafica '>

  
<div id="finanzas_chart" style="width: 560px; height: 300px;"></div>  
  
<table class="pretty">  
  <!-- Orders table code here -->  
</table>  
<script type="text/javascript" charset="utf-8">  
  $(function () {  
    new Highcharts.Chart({  
      chart: { renderTo: 'finanzas_chart' }  
    });  
  });

  $(function () {  
  new Highcharts.Chart({  
    chart: { renderTo: 'finanzas_chart' },  
    title: { text: 'Finanzas' },  
    xAxis: { type: 'datetime' },  
    yAxis: {  
      title: { text: 'Euros'}  
    },  

    tooltip: {  
      formatter: function () {  
        return Highcharts.dateFormat("%B %e %Y", this.x) + ': ' +  
          '$' + Highcharts.numberFormat(this.y, 2);  
      }  
    },      
    series: [{  
      pointInterval: <%= 1.day * 1000 %>,  
      pointStart: <%= Movement.first.created_at.to_i * 1000 %>,  
      data: <%= Movement.all.group("date(created_at)").sum(:amount).map{|k,v| v/100} %>
    }]  
  });  
});    
</script>  

</div>
</div>